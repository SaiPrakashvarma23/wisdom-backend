{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "strict": true,
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "esModuleInterop":true,
    "allowSyntheticDefaultImports": true,
    "outDir": "./dist"
  },
  "exclude": ["node_modules"]
}